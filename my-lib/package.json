{
  "name": "@my-lib/apple",
  "version": "1.0.0",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/cjs/index.js",
      "require": "./lib/esm/index.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --extensions '.ts' --out-dir 'lib/esm'",
    "build:cjs": "cross-env BABEL_ENV=cjsBundled babel src --extensions '.ts' --out-dir 'lib/cjs'"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/react": "^17.0.37",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.61.1",
    "typescript": "^4.5.3"
  },
  "license": "MIT"
}
